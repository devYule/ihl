SELECT TABLE_NAME FROM ALL_TABLES WHERE OWNER = ? AND TEMPORARY = 'N' AND PARTITIONED = 'NO' AND CLUSTER_NAME IS NULL AND TABLE_NAME NOT IN (SELECT VIEW_NAME FROM ALL_VIEWS WHERE OWNER = ?) AND TABLE_NAME NOT IN (SELECT MVIEW_NAME FROM ALL_MVIEWS WHERE OWNER = ?) AND TABLE_NAME NOT IN (SELECT SYNONYM_NAME FROM ALL_SYNONYMS WHERE OWNER = ?) AND TABLE_NAME NOT IN (SELECT OBJECT_NAME FROM USER_RECYCLEBIN)